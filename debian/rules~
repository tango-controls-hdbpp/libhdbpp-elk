#!/usr/bin/make -f

# See debhelper(8) (uncomment to enable)
# output every command that modifies files on the build system.
# export DH_VERBOSE = 1

%:
	dh $@

override_dh_auto_build:
	# work around for tango include peculiarities 
	dh_auto_build -- TANGO_INC=/usr/include/tango

override_dh_auto_install:
	dh_auto_install -- \
		DESTDIR=$(CURDIR)/debian/tmp \
		PREFIX=/usr

override_dh_auto_test:
